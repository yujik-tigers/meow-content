{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "FastAPI: Uvicorn",
        "type": "python",
        "request": "launch",
        "module": "uvicorn",
        "args": [
          "app.main:app",
          "--reload",
          "--port", "8000"
        ],
        "jinja": true,
        "env": {
          "PYTHONPATH": "${workspaceFolder}"
        },
        "console": "integratedTerminal",
        "justMyCode": false
      },
    ]
  }